                                            return false;
                                        }
                                        else {
                                            var reg = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[@$!%*?&])[A-Za-z\d@$!%*?&]{8,}$/;
                                            if (reg.test(password.value)) {
                                                document.getElementById("pass1").style.color = "green";
                                                document.getElementById("pass1").innerHTML = "Valid Password";
                                                return true;
                                            }
