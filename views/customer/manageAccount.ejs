                                            document.getElementById("pass3").style.color = "red";
                                            document.getElementById("pass3").innerHTML = "Password Required";
                                            return false;
                                        }
                                        else {
                                            var reg = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[@$!%*?&])[A-Za-z\d@$!%*?&]{8,}$/;
