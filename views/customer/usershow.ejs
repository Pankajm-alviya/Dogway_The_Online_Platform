               <div class="col-sm-6 mt-5">
                <h2 class="alert alert-danger text-center">List Of ServiesProvider</h2>
                <%#Show all Data %>
                    <table class="table table-striped table-center">
                        <thead>
                            <tr>
                                <th scope="col">No.</th>
                                <th scope="col">name</th>
                                <th scope="col">Contact</th>
                                <th scope="col">Email</th>
                                <th scope="col">Addharno.</th>
                                <th scope="col">Pancard No.</th>
                                <th scope="col">Address</th>
                                <th scope="col">Experience</th>
                                <th scope="col">type</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% var i=1 %>
                                <% data.forEach((item)=>{ %>
                                    <tr>
                                        <td>
                                            <%= i++%>
                                        </td>
                                        <td>
                                            <%= item.Name %>
                                        </td>
                                        <td>
                                            <%= item.Contact %>
                                        </td>
                                        <td>
                                            <%= item.Email %>
                                        </td>
                                        <td>
                                            <%= item.Addharno %>
                                        </td>
                                        <td>
                                            <%= item.Pancard %>
                                        </td>
                                        <td>
                                            <%= item.Address %>
                                        </td>
                                        <td>
                                            <%= item.Experience %>
                                        </td>
                                        <td>
                                            <%= item.type %>
                                        </td>
                                        <td>
                                            <form action="/edit/<%= item._id%>" method="get" class="d-inline">
                                                <button type="submit" class="btn"> <i class="fas fa-pen-square text-info"></i>
                                                </button>
                                            </form>
                                            <form action="/delete/<%= item._id%>" method="post" class="d-inline">
                                                <button type="submit" class="btn"> 
                                                <i class="fas fa-trash text-danger"></i>
                                                </button>
                                            </form>
                                        </td>
                                    </tr>
                                    <% })%>

                        </tbody>
                    </table>
            </div>

        </div>
    </div>

    <%- include('./partials/footer.ejs') %>